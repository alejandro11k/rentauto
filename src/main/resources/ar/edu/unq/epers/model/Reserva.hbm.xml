<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="ar.edu.unq.epers.model.Reserva" table="reservas">
		<id name="numeroSolicitud">
			<column name="numeroSolicitud" not-null="true"/>
			<generator class="identity" />
		</id>
		<many-to-one class="ar.edu.unq.epers.model.Ubicacion"
			name="origen" />
		<many-to-one class="ar.edu.unq.epers.model.Ubicacion"
			name="destino" />
		<many-to-one class="ar.edu.unq.epers.model.Usuario" name="usuario" />
		<many-to-one class="ar.edu.unq.epers.model.Auto" name="auto" />
		<property name="inicio" />
		<property name="fin" />
	</class>
</hibernate-mapping>