<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping>
    <class name="ar.edu.unq.epers.model.Auto" table="autos" >
        <id name="id" type="java.lang.Integer">
            <column name="J_ID" />
            <generator class="identity" />
        </id>

        <property name="marca" type="string">
            <column name="J_MARCA" length="20" not-null="true" unique="false" />
        </property>
                
        <property name="nombre" type="string">
            <column name="J_NOMBRE" length="20" not-null="true" unique="false" />
        </property>
        <property name="año" type="int">
            <column name="J_AÑO" not-null="false" unique="false"/>
        </property>
    </class>
</hibernate-mapping>